{
  "name": "Project Summer",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Add cultural and visual context to media online.",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus", "tabs"],
  "web_accessible_resources": ["*.html"]
}
